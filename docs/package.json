    "@mdx-js/react": "^1.6.22",
    "@mdx-js/runtime": "^1.6.22",
    "@reach/tabs": "^0.15.0",
    "@sentry/browser": "^5.6.1",
    "date-fns": "^2.28.0",
    "docsearch.js": "^2.5.2",
    "emoji-regex": "^9.2.2",
    "front-matter": "^2.3.0",
    "fs-extra": "^6.0.1",
    "github-slugger": "^1.3.0",
    "hotshot": "^1.0.5",
    "lodash": "^4.17.20",
    "next": "^12.2.2",
    "nprogress": "0.2.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "1.0.2",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-diff-view": "^2.4.7",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.9",
    "react-grid-system": "^8.1.3",
    "react-markdown": "^6.0.3",
